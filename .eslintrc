{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "strict": 0,
    "no-extra-strict": 0,
    "quotes": [2, "single", "avoid-escape"],
    "curly": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-unused-vars": [1, {"vars": "all", "args": "none"}],
    "no-multi-spaces": 0
  }
}